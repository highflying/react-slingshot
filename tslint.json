{
  "extends": ["tslint:recommended", "tslint-react", "tslint-eslint-rules"],
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "object-literal-sort-keys": false,
    "block-spacing": [
      true,
      "always"
    ],
    "no-unused-variable": true,
    "interface-name": [true, "never-prefix"]
  }
}
